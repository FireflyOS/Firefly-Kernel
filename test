CPP firefly/kernel/init/init.cpp
CPP firefly/kernel/memory-manager/secondary/slab/slab.cpp
CPP firefly/kernel/memory-manager/primary/primary_phys.cpp
CPP firefly/kernel/memory-manager/virtual/virtual.cpp
CPP firefly/kernel/console/console.cpp
CPP firefly/kernel/drivers/serial.cpp
CPP firefly/kernel/drivers/ps2.cpp
CPP firefly/kernel/intel64/gdt/tss.cpp
CPP firefly/kernel/intel64/gdt/gdt.cpp
CPP firefly/kernel/intel64/cpu/cpu.cpp
CPP firefly/kernel/intel64/int/interrupt.cpp
CPP firefly/kernel/intel64/paging.cpp
CPP firefly/kernel/stubs.cpp
CPP firefly/kernel/kernel.cpp
CPP firefly/kernel/trace/strace.cpp
CPP firefly/kernel/trace/symbols.cpp
CPP firefly/libk++/fmt.cpp
CPP firefly/libk++/memory.cpp
CPP firefly/libk++/utils.cpp
CPP include/cstdlib/cmath.cpp
CPP include/cstdlib/cstd.cpp
AS firefly/kernel/intel64/gdt/_gdt.asm
AS firefly/kernel/intel64/int/_interrupt.asm
CC firefly/kernel/console/limine-terminal-port/image.c
CC firefly/kernel/console/limine-terminal-port/term.c
CC firefly/kernel/console/limine-terminal-port/tterm.c
CC firefly/kernel/console/limine-terminal-port/gterm.c
echo '#include <symbols.hpp>' > parsed_x86_64.sym
echo 'SymbolTablePair symbol_table[] = {{0xFFFFFFFFFFFFFFFF, ""}};' >> parsed_x86_64.sym
clang++ -x c++ -I include/firefly/trace -m64 -c parsed_x86_64.sym -o symtable.o
ld.lld -nostdlib -T linkage/linker_x86_64.ld -static -z max-page-size=0x1000  firefly/kernel/init/init.o firefly/kernel/memory-manager/secondary/slab/slab.o firefly/kernel/memory-manager/primary/primary_phys.o firefly/kernel/memory-manager/virtual/virtual.o firefly/kernel/console/console.o firefly/kernel/drivers/serial.o firefly/kernel/drivers/ps2.o firefly/kernel/intel64/gdt/tss.o firefly/kernel/intel64/gdt/gdt.o firefly/kernel/intel64/cpu/cpu.o firefly/kernel/intel64/int/interrupt.o firefly/kernel/intel64/paging.o firefly/kernel/stubs.o firefly/kernel/kernel.o firefly/kernel/trace/strace.o firefly/kernel/trace/symbols.o firefly/libk++/fmt.o firefly/libk++/memory.o firefly/libk++/utils.o include/cstdlib/cmath.o include/cstdlib/cstd.o firefly/kernel/intel64/gdt/_gdt.o firefly/kernel/intel64/int/_interrupt.o firefly/kernel/console/limine-terminal-port/image.o firefly/kernel/console/limine-terminal-port/term.o firefly/kernel/console/limine-terminal-port/tterm.o firefly/kernel/console/limine-terminal-port/gterm.o font.o symtable.o -o hypervisor-x86_64.elf
python3 scripts/symbol_table_x86_64.py hypervisor-x86_64.elf
[*] Wrote symbol table
[*] Compiled symbol table
[*] Relinked kernel
./scripts/geniso.sh
Built hypervisor
